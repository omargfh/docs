fault”.",[51,91,92],{},"Note that specifying a default shape does not prevent you from\nchanging an agent’s shape later. Agents don’t have to be\nstuck with their breed’s default shape.",[94,95,96],"pre",{},[97,98,99,75,105,75,110,115,118,75,123,126,127,130,132,134,75,137,75,142,146,148,75,151,75,154,158,160,75,163,126,166,169,171,173,75,176,75,180,184,186,75,189,75,193,197,199,75,202,205,206,210,212,75,216,219,220,224,75,229,232,233,235,236,240,235,242],"code",{},[28,100,104],{"className":101},[102,103],"token","command","create-turtles",[28,106,109],{"className":107},[102,108],"number","1",[28,111,114],{"className":112},[102,113],"comment",";; new turtle's shape is \"default\"",[116,117],"br",{},[28,119,122],{"className":120},[102,121],"variable","create-cats",[28,124,109],{"className":125},[102,108],"    ",[28,128,114],{"className":129},[102,113],[116,131],{},[116,133],{},[28,135,14],{"className":136},[102,103],[28,138,141],{"className":139},[102,140],"reporter","turtles",[28,143,145],{"className":144},[102,57],"\"circle\"",[116,147],{},[28,149,104],{"className":150},[102,103],[28,152,109],{"className":153},[102,108],[28,155,157],{"className":156},[102,113],";; new turtle's shape is \"circle\"",[116,159],{},[28,161,122],{"className":162},[102,121],[28,164,109],{"className":165},[102,108],[28,167,157],{"className":168},[102,113],[116,170],{},[116,172],{},[28,174,14],{"className":175},[102,103],[28,177,179],{"className":178},[102,121],"cats",[28,181,183],{"className":182},[102,57],"\"cat\"",[116,185],{},[28,187,14],{"className":188},[102,103],[28,190,192],{"className":191},[102,121],"dogs",[28,194,196],{"className":195},[102,57],"\"dog\"",[116,198],{},[28,200,122],{"className":201},[102,121],[28,203,109],{"className":204},[102,108],"   ",[28,207,209],{"className":208},[102,113],";; new turtle's shape is \"cat\"",[116,211],{},[28,213,215],{"className":214},[102,103],"ask",[28,217,179],{"className":218},[102,121]," [ ",[28,221,223],{"className":222},[102,103],"set",[